  Vcl.Forms,
  Main in 'Main.pas' {MainForm},
  Lib in 'lib\Lib.pas',
  JavascriptObject in 'lib\JavascriptObject.pas',
  uPSI_JavascriptObject in 'lib\uPSI_JavascriptObject.pas',
